<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="milesapnash.astrostudy.SelectTopicViewController">
    <Label id="selectedButton" prefHeight="100.0" prefWidth="400.0" text="Choose topic"/>
    <ComboBox fx:id="topicBox" layoutX="124.0" layoutY="174.0" prefWidth="150.0" promptText="Topic"
              visibleRowCount="3"/>
    <Button layoutX="44.0" layoutY="301.0" mnemonicParsing="false" prefHeight="48.0"
            prefWidth="311.0" text="Start Test" onAction="#startTopicTest"/>
</AnchorPane>
