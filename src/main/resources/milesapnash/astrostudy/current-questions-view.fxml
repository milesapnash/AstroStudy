            fx:controller="milesapnash.astrostudy.CurrentQuestionsController">
    <Label prefHeight="100.0" prefWidth="400.0" text="Questions"/>
    <Button fx:id="newQuestion" layoutX="275.0" layoutY="90.0" mnemonicParsing="false"
            onAction="#toNew" text="ðŸ•® New" visible="false"/>
    <Button id="selectedButton" mnemonicParsing="false" onAction="#toMenu" prefHeight="35.0"
            prefWidth="50.0" text="ðŸ ”"/>
    <ComboBox fx:id="topicBox" layoutX="25.0" layoutY="100.0" prefWidth="150.0" promptText="Topic"
