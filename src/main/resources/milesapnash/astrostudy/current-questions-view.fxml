    <ListView fx:id="questionList" layoutX="25.0" layoutY="150.0" prefHeight="150.0"
              prefWidth="350.0" visible="false"/>
    <Button fx:id="editQuestion" layoutX="50.0" layoutY="325.0" mnemonicParsing="false"
            onAction="#toEdit" prefHeight="25.0" prefWidth="125.0" text="Edit" visible="false"/>
    <Button fx:id="deleteQuestion" layoutX="225.0" layoutY="325.0" mnemonicParsing="false"
            onAction="#delete" prefHeight="25.0" prefWidth="125.0" text="Delete" visible="false"/>
